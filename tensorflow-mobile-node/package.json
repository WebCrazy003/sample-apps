{
  "name": "TensorFlow-Mobile-Node-App",
  "version": "0.0.2",
  "description": "Image Recognition using TensorFlow Mobile",
  "author": "OOMWOO LLC",
  "license": "Apache-2.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "clean": "rm -rf public/bundle.js",
    "clean-all": "rm -rf public/bundle.js node_modules"
  },
  "dependencies": {
    "idb-keyval": "^3.1.0",
    "kaia.js": "^0.0.8"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "esmodules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 3 versions",
              "ie >= 9",
              "ios >= 8",
              "android >= 4.2"
            ]
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  }
}
